{
  "name": "upax-front-users",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start:dev": "webpack serve --config config/webpack.dev.js",
    "start:qa": "webpack serve --config config/webpack.qa.js",
    "start:beta": "cross-env REACT_APP_ENV=beta && webpack serve --config config/webpack.beta.js",
    "start:prod": "cross-env REACT_APP_ENV=prod && webpack serve --config config/webpack.prod.js",
    "build:dev": "cross-env REACT_APP_ENV=dev NODE_ENV=production webpack --config config/webpack.dev.js",
    "build:qa": "cross-env REACT_APP_ENV=qa NODE_ENV=production webpack --config config/webpack.qa.js",
    "build:beta": "cross-env REACT_APP_ENV=beta NODE_ENV=production webpack --config config/webpack.beta.js",
    "build:prod": "cross-env REACT_APP_ENV=prod NODE_ENV=production webpack --config config/webpack.prod.js",
    "serve": "serve dist -p 3002",
    "clean:windows": "del /f package-lock.json && rmdir /s node_modules",
    "clean:linux": "rm package-lock.json && rm -r node_modules",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "deploy": "npm run publish && npm run pack",
    "publish": "jf npm p --build-name=$APP_NAME --build-number=$APP_VERSION",
    "pack": "jf npm install --prod --no-optional --no-fund"
  },
  "dependencies": {
    "axios": "1.7.9",
    "cross-env": "7.0.3",
    "dotenv": "16.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.28.1",
    "react-lottie": "1.2.10",
    "zustand": "5.0.3"
  },
  "devDependencies": {
